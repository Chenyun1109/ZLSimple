<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="develop.y.zhzl">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="framework.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".main.widget.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".detail.widget.DetailActivity" />
        <activity android:name=".main.widget.AboutActivity" />
        <activity android:name=".search.widget.AnnalActivity" />
        <activity
            android:name=".main.widget.TransitionActivity"
            android:theme="@style/Theme" />
    </application>

</manifest>